<h1>Listing feeds</h1>

<table>
  <tr>
    <th>Url</th>
    <th>Actions</th>
  </tr>

<% @feeds.each do |feed| %>
  <tr>
    <td>
     <%= link_to image_tag("/images/rss.jpeg",{:height => "20px",:width => "40px"}), feed.url %>
     </td>
     <td> <%= link_to 'Show', feed %> | <%= link_to 'Edit', edit_feed_path(feed) %> |
     <%= link_to 'Destroy', feed, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Feed', new_feed_path %>
